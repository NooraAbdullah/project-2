<p id="notice"><%= notice %></p>

<h1>Venues</h1>

<table class="table">
  <thead class="thead-dark">
    <tr class="firstRow">
      <th>Name</th>
      <th>City</th>
      <th>Price</th>
      <th>Capacity</th>
      <th>Phone</th>
      <th>Image</th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr class="rows">
        <td><%= venue.name %></td> 
        <td><%= venue.city %></td>
        <td><%= venue.price %></td>
        <td class="showDest" ><%= link_to 'More info', venue %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<br>
<%= link_to 'Back', root_path %>
